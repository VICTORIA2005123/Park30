{
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "database": {
    "rules": "database.rules.json"
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "marwadiparking",
          "appId": "1:752207806635:android:6f41b5f0a08b76db69b7ee",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "marwadiparking",
          "configurations": {
            "android": "1:752207806635:android:6f41b5f0a08b76db69b7ee",
            "ios": "1:752207806635:ios:ea809728f0c2a73969b7ee",
            "macos": "1:752207806635:ios:ea809728f0c2a73969b7ee",
            "web": "1:752207806635:web:434c96cc52dfed8b69b7ee",
            "windows": "1:752207806635:web:30aa6d1e994dcc9f69b7ee"
          }
        }
      }
    }
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
